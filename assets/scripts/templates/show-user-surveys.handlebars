

<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
  {{#each surveys }}
    <div class="panel panel-default">
      <div class="panel-heading" role="tab" id="heading{{this.id}}">
        <h4 class="panel-title">
          <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse{{this.id}}" aria-expanded="true" aria-controls="collapseOne">
            {{ this.question }}
          </a>
        </h4>
      </div>
      <div id="collapse{{this.id}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading{{this.id}}">
        <div class="panel-body">
          {{ this.options }}
        </div>
        <button class="btn btn-danger delete-survey" data-target="{{ this.id }}">Delete Survey</button>
      </div>
    </div>
    {{/each}}
</div>
